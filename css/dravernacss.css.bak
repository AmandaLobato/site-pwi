        @charset "UTF-8";
        @import url('https://fonts.googleapis.com/css2?family=Marcellus+SC&family=Marck+Script&display=swap');
        @import url('https://fonts.googleapis.com/css2?family=Marcellus+SC&family=Marck+Script&family=Quintessential&family=Trocchi&display=swap');
        @import url('https://fonts.googleapis.com/css2?family=Marcellus+SC&family=Marck+Script&family=Quintessential&family=Trocchi&display=swap');
        @import url('https://fonts.googleapis.com/css2?family=Kurale&family=Marcellus+SC&family=Marck+Script&family=Quintessential&family=Trocchi&display=swap');
        @import url('https://fonts.googleapis.com/css2?family=IM+Fell+English&family=Kurale&family=Marcellus+SC&family=Marck+Script&family=Quintessential&family=Trocchi&display=swap');
        @import url('https://fonts.googleapis.com/css2?family=IM+Fell+English&family=Joan&family=Kurale&family=Marcellus+SC&family=Marck+Script&family=Quintessential&family=Trocchi&display=swap');
        @import url('https://fonts.googleapis.com/css2?family=IM+Fell+English&family=Joan&family=Kurale&family=Marcellus+SC&family=Marck+Script&family=Merriweather:wght@300&family=Quintessential&family=Trocchi&display=swap');

        * {
            margin: 0px;
            padding: 0px;
        }

        :root {
            --cor0: #F7BE14;
            --cor1: #F80505;
            --cor2: #FA05A8;
            --cor3: #650489;
            --cor4: #19034D;
            --cor5: #F4E69D;
            --cor6: #FF6666;
            --cor7: #1f1f1f;
            --cor8: #fdcc39;
            --corheader: #d13e3e;
            --cormain: #ffd043;
            --corbody: #f3bb23;
            --font-padrao: Arial, Verdana, Helvetica, sans-serif;
            --font-titulo: 'Kurale', serif;
            --font-sub: : 'IM Fell English', serif;
            --font-text: 'Joan', serif;
            --font-text2: 'Marcellus SC', serif;
            --font-text3: 'Marck Script', cursive;
            --font-text4: 'Quintessential', cursive;
            --font-text5: 'Trocchi', serif;
            --font-draverna: 'Merriweather', serif;
        }

        body {
            background: #f3bb23;
        }

        body,
        ul {
            margin: 0px;
            padding: 0px;
        }

        header {
            min-height: 90px;
            top: 0px;
            position: fixed;
            width: 100%;
            background-color: #d13e3e;
            z-index: 19999;
            padding-top: 0px;
            padding-bottom: 0%;
            margin-bottom: 0px;
        }

        header img {
            padding-bottom: 0%;
        }

        .nav-list a:hover {
            display: block;
            background-color: #ad3131;
            padding: 0.5rem;
            padding: 15px;
            border-radius: 3px;
        }

        .nav-bar {
            display: flex;
            justify-content: space-between;
            padding: 1rem 6rem;
        }

        .logo {
            display: flex;
            align-items: center;
        }

        .nav-list {
            display: flex;
            align-items: center;
        }

        .nav-list ul {
            display: flex;
            justify-content: center;
            list-style: none;
        }

        .nav-item {
            margin: 0px 15px;
        }

        .nav-link {
            text-decoration: none;
            font-size: 1.15rem;
            color: aliceblue;
        }

        .mobile-menu-icon {
            display: none;
        }

        .mobile-menu {
            display: none;
        }

        @media screen and (max-width: 650px) {
            .nav-bar {
                padding: 1.5rem 4rem;
            }

            .nav-item {
                display: none;
            }

            .mobile-menu-icon {
                display: block;
                margin-top: 25px;
            }

            .mobile-menu-icon button {
                background-color: transparent;
                border: none;
                cursor: pointer;
            }

            .mobile-menu ul {
                display: flex;
                flex-direction: column;
                text-align: center;
                padding-bottom: 1rem;
            }

            .mobile-menu .nav-item {
                display: block;
                padding-top: 1.2rem;
            }

            .open {
                display: block;
            }
        }

        u {
            margin-top: 0;
            background-color: black;
        }

        main {
            min-width: 300px;
            max-width: 1000px;
            margin: 0;
            margin-bottom: 0px;
            margin-top: 10rem;
            padding: 45px 60px;
            margin-left: auto;
            margin-right: auto;
            align-items: center;
            background-color: #ffd043;
            box-shadow: #19034D;
            border-bottom-left-radius: 25px;
            border-bottom-right-radius: 25px;

        }

        .slogan h2 {
            color: var(--corheader);
        }

        main h2 {
            color: var(--corheader);
            font-family: var (--font-text3);
            text-decoration: none;
            margin: 15px;
            text-align: center;
            font-size: 40px;
        }

        main img {
            width: 20rem;
            height: 20rem;
            object-fit: cover;
        }

        main img.dec1 {
            display: block;
            margin: auto;
        }

        .rolagem {
            box-shadow: 2px 2px 4px black;
            overflow: hidden;
            height: 500px;
            width: 500px;

        }

        .carrossel {
            width: 1000px;
            height: 300px;
            overflow: hidden;
            margin: 50px auto;
            display: block;
            max-width: 100%;
        }

        .carrossel ul {
            display: flex;
            list-style: none;
            width: 1200px;
            height: 800px;
        }

        .carrossel li {
            position: relative;
            animation: slide 15s infinite ease-out;
        }

        @keyframes slide {
            0% {
                left: 0px
            }

            20% {
                left: 0px
            }

            30% {
                left: -100px
            }

            40% {
                left: -200px
            }

            50% {
                left: -250px
            }

            60% {
                left: -300px
            }

            70% {
                left: -350px
            }

            80% {
                left: -400px
            }

            90% {
                left: -450px
            }

            100% {
                left: -200px
            }

            100% {
                right: -200px
            }

            90% {
                right: -250px
            }

            70% {
                right: -300px
            }

            60% {
                right: -350px
            }

            40% {
                right: -400px
            }

            20% {
                right: -450px
            }

            10% {
                right: -500px
            }

            0% {
                right: -550px
            }
        }

        .promocao h2 {
            color: azure;
            display: block;
            width: 100%;
            height: 120%;
            background-color: #31cfff;
            margin: 25px;
            margin: auto;
        }

        .promocao img {
            display: block;
            width: 100%;
            height: 120%;
            background-color: #31cfff;
            margin: 25px;
            margin: auto;

        }

        .promocao img:hover {
            opacity: 80%;
        }

        .promocao {
            text-align: center;
            padding: 0PX 0px;
            margin: 0PX 0PX;
            padding-top: 15PX;
            display: block;
            width: 100%;
            height: 120%;
            background-color: #31cfff;
            margin: 25px;
            margin: auto;

        }

        .historia {
            margin-top: 90px;

        }

        main p {
            font-family: var(--font-draverna);
            line-height: 2em;
            color: rgb(0, 0, 0);
            margin: 15px 0px;

            text-align: center;
        }

        h2 {
            margin: 15px;
            text-align: center;
            font-size: 40px;
        }

        footer {
            background-color: var(--corheader);
            margin: auto;
            padding: 20px 20px;

            width: 100%;
            bottom: 0;
            left: 0;
        }

        p {
            font-family: var(--font-text);
            font-style: bold;
            color: rgb(255, 255, 255);
            text-align: center;
            padding: 25px 25px;
            margin: auto;
        }
.rodape {
    max-width: 1440px;
    position: relative;
    z-index: 1;
    margin: 0 auto;
    width: calc(100% - 40px);
}
ul {
    display: block;
    list-style-type: disc;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    padding-inline-start: 40px;
}
